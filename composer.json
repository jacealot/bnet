{
    "name": "boo/bnet",
    "description": "Library for working with the Battle.net Web APIs.",
    "keywords": ["Battle.net", "Blizzard", "Warcraft", "Starcraft", "Diablo", "Overwatch", "API", "bnet"],
    "homepage": "https://github.com/jyggen/bnet",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
    ],
    "support": {
        "docs": "https://docs.jyggen.com/boo-bnet/latest",
        "issues": "https://github.com/jyggen/bnet/issues",
        "source": "https://github.com/jyggen/bnet"
   },
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "league/oauth2-client": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "ext-json": "*",
        "boo/code-standard": "^0.1.1",
        "guzzlehttp/guzzle": "^6.0",
        "http-interop/http-factory-guzzle": "^1.0.0",
        "phpunit/phpunit": "^6.2.4",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/var-dumper": "^4.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Boo\\BattleNet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boo\\BattleNet\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "test": [
            "mkdir -p build/logs",
            "grumphp run"
        ]
    }
}
