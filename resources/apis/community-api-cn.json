{
  "name": "Community APIs - CN",
  "title": "Community APIs - CN",
  "description": "China-specific endpoints for StarCraft II and Diablo III profiles.\n\n**Diablo III Icons:** Developers can fetch the images referenced by the D3 APIs with this URL template: `http://media.blizzard.com/d3/icons/<type>/<size>/<icon>.png`.\n\n- `<type>` is on the type of resource referenced; it can either be `items` or `skills`.\n- `<size>` for *items* must either be `small` or `large`.\n- `<size>` for *skills* must be `21`, `42`, or `64`. These values refer to the image size in pixels.\n\n**Hosts**: note that Blizzard API request paths in China follow this format: `https://gateway.battlenet.com.cn` and that OAuth requests in China use `www.battlenet.com.cn/{API path}`. For more information, see the [Getting Started guide](/documentation/guides/getting-started).",
  "version": "78",
  "basePath": "https://gateway.battlenet.com.cn",
  "protocol": "rest",
  "headers": {
    "Accept": "application/json"
  },
  "auth": {
    "oauth": {
      "version": "2.0",
      "base_uri": "https://www.battlenet.com.cn",
      "authorize_uri": "/oauth/authorize",
      "access_token_uri": "/oauth/token",
      "auth_flows": [
        "client_cred"
      ],
      "options": {
        "authorize": {
          "scope": []
        }
      }
    }
  },
  "resources": {
    "Sc2 Profile API": {
      "methods": {
        "Profile": {
          "description": "Returns data about an individual SC2 profile.",
          "path": "/sc2/profile/:id/:region/:name/",
          "parameters": {
            ":id": {
              "type": "string",
              "required": "true",
              "description": "The ID of the profile to retrieve.",
              "location": "pathReplace"
            },
            ":region": {
              "type": "string",
              "required": "true",
              "default": "1",
              "description": "The region of the profile to retrieve.",
              "location": "pathReplace"
            },
            ":name": {
              "type": "string",
              "required": "true",
              "description": "The name of the profile to retrieve.",
              "location": "pathReplace"
            },
            "locale": {
              "type": "string",
              "required": false,
              "default": "zh_CN",
              "enum": [
                "zh_CN"
              ],
              "description": "The locale to use in the response.",
              "location": "query"
            },
            "callback": {
              "type": "string",
              "required": false,
              "default": "",
              "description": "Request for the data to be returned as a JSONP callback.",
              "location": "query"
            }
          },
          "httpMethod": "GET"
        },
        "Ladders": {
          "description": "Returns data about an individual SC2 profile's ladders.",
          "path": "/sc2/profile/:id/:region/:name/ladders",
          "parameters": {
            ":id": {
